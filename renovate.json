{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "github>whitesource/merge-confidence:beta",
    ":prHourlyLimitNone",
    ":rebaseStalePrs",
    ":label(renovate)",
    ":preserveSemverRanges"
  ],
  "enabledManagers": [
    "composer"
  ],
  "baseBranches": [
    "develop"
  ],
  "rangeStrategy": "update-lockfile",
  "rebaseWhen": "conflicted",
  "configMigration": true
}
