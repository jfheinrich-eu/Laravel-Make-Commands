{
  "name": "jfheinrich-eu/laravel-make-commands",
  "description": "This package is aimed to be a suite of artisan commands and tools to help make the work easier.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://packagist.org/packages/jfheinrich-eu/laravel-make-commands",
  "autoload": {
    "psr-4": {
      "JfheinrichEu\\LaravelMakeCommands\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JfheinrichEu\\LaravelMakeCommands\\Tests\\": "tests/"
    }
  },
  "authors": [{
    "name": "Joerg Heinrich",
    "homepage": "https://jfheinrich.eu",
    "email": "contact@jfheinrich.eu",
    "role": "Owner"
  }],
  "minimum-stability": "dev",
  "scripts": {
    "test": "./vendor/bin/pest",
    "test:coverage": "env XDEBUG_MODE=coverage ./vendor/bin/pest --coverage  --colors=never",
    "fix": "./vendor/bin/pint",
    "check": "./vendor/bin/pint --test -v",
    "static": "./vendor/bin/phpstan analyse",
    "all": [
      "@test",
      "@fix",
      "@check",
      "@static"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "JfheinrichEu\\LaravelMakeCommands\\Providers\\PackageServiceProvider"
      ],
      "aliases": [
        "JfHeinrichEu\\LaravelMakeCommands\\Facades\\Hydrator"
      ]
    }
  },
  "prefer-stable": true,
  "config": {
    "platform": {
      "php": "8.1"
    },
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.1",
    "orchestra/testbench": "^7.6",
    "pestphp/pest": "^1.21",
    "pestphp/pest-plugin-mock": "^1.0",
    "phpstan/phpstan": "^1.8"
  },
  "require": {
    "eventsauce/object-hydrator": "^1.0",
    "illuminate/console": "^9.39",
    "illuminate/support": "^9.40"
  }
}
