{
  "name": "jfheinrich-eu/laravel-make-commands",
  "description": "A set of tools to make working with Data Transfer Objects easier in Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [{
    "role": "Developer",
    "name": "Joerg Heinrich",
    "email": "contact@jfheinrich.eu",
    "homepage": "https://www.Jfheinrich.eu/"
  }],
  "autoload": {
    "psr-4": {
      "Jfheinrich\\DataObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jfheinrich\\DataObjects\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "eventsauce/object-hydrator": "^0.4.3",
    "illuminate/contracts": "^9.22",
    "illuminate/support": "^9.22"
  },
  "scripts": {
    "test": "./vendor/bin/pest",
    "test:coverage": "env XDEBUG_MODE=coverage ./vendor/bin/pest --coverage",
    "fix": "./vendor/bin/pint",
    "check": "./vendor/bin/pint --test -v",
    "static": "./vendor/bin/phpstan analyse",
    "all": [
      "@test",
      "@fix",
      "@check",
      "@static"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jfheinrich\\DataObjects\\Providers\\PackageServiceProvider"
      ],
      "aliases": [
        "Jfheinrich\\DataObjects\\Facades\\Hydrator"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "require-dev": {
    "laravel/pint": "^1.1",
    "orchestra/testbench": "^7.6",
    "pestphp/pest": "^1.21",
    "phpstan/phpstan": "^1.8"
  }
}
